        "dist/js/bootstrap.min.js.map",
        "scss/bootstrap-dark.scss",
        "scss/bootstrap-light.scss",
        "scss/bootstrap-prefers-dark.scss",
        "scss/bootstrap-prefers-light.scss",
        "scss/toggle-bootstrap-dark.scss",
        "scss/toggle-bootstrap-print.scss",
        "scss/toggle-bootstrap.scss",
        "scss/_dark-input-group.scss",
