        "dist/css/bootstrap-dark-prefers-light.css",
        "dist/css/bootstrap-dark-prefers-light.min.css",
        "dist/css/bootstrap-dark.css",
        "dist/css/bootstrap-dark.min.css",
        "dist/css/bootstrap-light-prefers-dark.css",
        "dist/css/bootstrap-light-prefers-dark.min.css",
        "dist/css/bootstrap-light.css",
        "dist/css/bootstrap-light.min.css",
        "dist/css/bootstrap-prefers-dark-color-only.css",
        "dist/css/bootstrap-prefers-dark-color-only.min.css",
        "dist/css/bootstrap-prefers-dark.css",
        "dist/css/bootstrap-prefers-dark.min.css",
        "dist/css/bootstrap-prefers-light-color-only.css",
        "dist/css/bootstrap-prefers-light-color-only.min.css",
        "dist/css/bootstrap-prefers-light.css",
        "dist/css/bootstrap-prefers-light.min.css",
        "dist/css/toggle-bootstrap-dark.css",
        "dist/css/toggle-bootstrap-dark.min.css",
