{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr",
      "files": [
        "dist/browser/signalr.js",
        "package.json",
        "src/third-party-notices.txt"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "@forevolve/bootstrap-dark@2.1.0",
      "destination": "wwwroot/lib/forevolve/bootstrap-dark/",
      "files": [
        "dist/css/toggle-bootstrap-dark.min.css",
        "dist/css/toggle-bootstrap.min.css",
        "LICENSE",
        "package.json"
      ]
    }
  ]
}